hostname cust-d-01
!
ip cef
!
interface Loopback0
 ip address 192.168.4.1 255.255.255.0
 no shutdown
!
interface Ethernet1/1
 no switchport
 ip address 172.16.0.2 255.255.255.252
 no shutdown
!
router bgp 1340
 bgp log-neighbor-changes
 neighbor 172.16.0.1 remote-as 2
 !
 address-family ipv4
  network 192.168.6.0
  neighbor 172.16.0.1 activate
 exit-address-family
!
