hostname cust-d-02
!
ip cef
!
interface Loopback0
 ip address 192.168.5.1 255.255.255.0
 no shutdown
!
interface Ethernet1/1
 no switchport
 ip address 172.16.0.6 255.255.255.252
 no shutdown
!
router bgp 1340
 bgp log-neighbor-changes
 neighbor 2.255.0.3 remote-as 2
 neighbor 2.255.0.3 update-source Loopback0
 !
 address-family ipv4
  network 192.168.5.0
  neighbor 2.255.0.3 activate
 exit-address-family
!
