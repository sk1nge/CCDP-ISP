hostname cust-d-03
!
interface Loopback0
 ip address 192.168.6.1 255.255.255.0
 no shutdown
!
interface FastEthernet0/0
 ip address 172.16.0.10 255.255.255.252
 no shutdown
!
router bgp 1340
 bgp log-neighbor-changes
 neighbor 172.16.0.9 remote-as 2
 !
 address-family ipv4
  network 192.168.6.0
  neighbor 172.16.0.9 activate
 exit-address-family
!
